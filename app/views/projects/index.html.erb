<% reset_cycle %>
<table class="list">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Description</th>
      <th>Created on</th>
      <th>Updated on</th>
    </tr>
  </thead>
  <tbody>
  <% @projects.each do |p| %>
    <tr class="<%= cycle("odd", "even") %>">
      <td><%= p.id %></td>
      <td><%= p.name %></td>
      <td><%= p.description %></td>
      <td><%= p.created_on.strftime("%F") %></td>
      <td><%= p.updated_on.strftime("%F") %></td>
    </tr>
  <% end %>
  </tbody>
<table>
